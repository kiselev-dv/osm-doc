<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <trait name="drive_in" group-tags="false">
        <title lang="ru" key="l10n.trait.drive_in.title">Обслуживание из машины</title>
        <title lang="en" key="l10n.trait.drive_in.title">Drivein</title>
        <more-tags>
            <tag tag-value-type="boolean">
                <title lang="ru" key="l10n.trait.drive_in.more_tags.drive_in.title">Обслуживание не выходя из машины</title>
                <title lang="en" key="l10n.trait.drive_in.more_tags.drive_in.title">Drive-in</title>
                <key>drive_in</key>
                <val value="true">
                    <title lang="ru" key="l10n.trait.drive_in.more_tags.drive_in.true.title">Доступно</title>
                    <title lang="en" key="l10n.trait.drive_in.more_tags.drive_in.true.title">Yes</title>
                </val>
                <val value="false">
                    <title lang="ru" key="l10n.trait.drive_in.more_tags.drive_in.false.title">Недоступно</title>
                    <title lang="en" key="l10n.trait.drive_in.more_tags.drive_in.false.title">No</title>
                </val>
            </tag>
        </more-tags>
    </trait>
    
    <trait name="drive_through">
        <title lang="ru">Обслуживание из машины</title>
        <title lang="en">Drive through</title>
        <more-tags>
            <tag tag-value-type="boolean">
                <title lang="ru">Обслуживание не выходя из машины</title>
                <title lang="en">Drive-in</title>
                <key>drive_through</key>
                <val value="true">
                    <title lang="ru">Доступно</title>
                    <title lang="en">Yes</title>
                </val>
                <val value="false">
                    <title lang="ru">Недоступно</title>
                    <title lang="en">No</title>
                </val>
            </tag>
        </more-tags>
    </trait>
    
</doc-part>
