<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <feature name="hospital">
        <title lang="ru" key="l10n.feature.hospital.title">Больница</title>
        <title lang="en" key="l10n.feature.hospital.title">Hospital</title>
        <tags>
            <tag>
                <key>amenity</key>
                <val value="hospital"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <trait>accessability</trait>
        <more-tags>
            <tag tag-value-type="boolean">
                <title lang="ru" key="l10n.feature.hospital.more_tags.emergency.title">Неотложная помощь</title>
                <title lang="en" key="l10n.feature.hospital.more_tags.emergency.title">Emergency</title>
                <key>emergency</key>
                <val value="true">
                    <title lang="ru" key="l10n.feature.hospital.more_tags.emergency.true.title">Да</title>
                    <title lang="en" key="l10n.feature.hospital.more_tags.emergency.true.title">Yes</title>
                </val>
                <val value="false">
                    <title lang="ru" key="l10n.feature.hospital.more_tags.emergency.false.title">Нет</title>
                    <title lang="en" key="l10n.feature.hospital.more_tags.emergency.false.title">No</title>
                </val>
            </tag>
        </more-tags>
        <icon>hospital.png</icon>
        <applyed-to>node area</applyed-to>
    </feature>
</doc-part>
