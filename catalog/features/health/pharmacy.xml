<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <feature name="pharmacy">
        <title lang="ru" key="l10n.feature.pharmacy.title">Аптека</title>
        <title lang="en" key="l10n.feature.pharmacy.title">Pharmacy</title>
        <tags>
            <tag>
                <key>amenity</key>
                <val value="pharmacy"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <trait>accessability</trait>
        <more-tags>
            <tag tag-value-type="boolean">
                <title lang="ru" key="l10n.feature.pharmacy.more_tags.dispensing.title">Изготовление лекарств по рецепту</title>
                <title lang="en" key="l10n.feature.pharmacy.more_tags.dispensing.title">Dispensing</title>
                <key>dispensing</key>
                <val value="true">
                    <title lang="ru" key="l10n.feature.pharmacy.more_tags.dispensing.true.title">Да</title>
                    <title lang="en" key="l10n.feature.pharmacy.more_tags.dispensing.true.title">Yes</title>
                </val>
                <val value="false">
                    <title lang="ru" key="l10n.feature.pharmacy.more_tags.dispensing.false.title">Нет</title>
                    <title lang="en" key="l10n.feature.pharmacy.more_tags.dispensing.false.title">No</title>
                </val>
            </tag>
        </more-tags>
        <icon>pharmacy.png</icon>
        <applyed-to>node area</applyed-to>
    </feature>
</doc-part>
