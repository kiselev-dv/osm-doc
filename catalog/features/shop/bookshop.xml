<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <feature name="bookshop">
        <title lang="ru" key="l10n.feature.bookshop.title">Книжный</title>
        <title lang="en" key="l10n.feature.bookshop.title">Bookshop</title>
        <tags>
            <tag>
                <key>shop</key>
                <val value="books"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <more-tags>
            <tag tag-value-type="enum">
                <title lang="ru" key="l10n.feature.bookshop.more_tags.second_hand.title">Продажа б/у</title>
                <title lang="en" key="l10n.feature.bookshop.more_tags.second_hand.title">Second hand</title>
                <key>second_hand</key>
                <val value="yes">
                    <title lang="ru" key="l10n.more_tags.true">Да</title>
                    <title lang="en" key="l10n.more_tags.true">Yes</title>
                </val>
                <val value="no">
                    <title lang="ru" key="l10n.more_tags.false">Нет</title>
                    <title lang="en" key="l10n.more_tags.false">No</title>
                </val>
                <val value="only">
                    <title lang="ru" key="l10n.feature.bookshop.more_tags.second_hand.only.title">только б/у</title>
                    <title lang="en" key="l10n.feature.bookshop.more_tags.second_hand.only.title">Only</title>
                </val>
            </tag>
        </more-tags>
        <icon>bookshop.png</icon>
        <applyed-to>node area</applyed-to>
    </feature>
</doc-part>
