<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <feature name="restaurant">
        <title lang="ru" key="l10n.feature.restaurant.title">Ресторан</title>
        <title lang="en" key="l10n.feature.restaurant.title">Restaurant</title>
        <tags>
            <tag>
                <key>amenity</key>
                <val value="restaurant"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <trait>seasonal</trait>
        <trait>cuisine</trait>
        <trait>parking</trait>
        <trait>internet-access</trait>
        <trait>smoking</trait>
        <trait>accessability</trait>
        <trait>drive_in</trait>
        <more-tags>
            <tag tag-value-type="boolean">
                <title lang="ru" key="l10n.feature.bar.more_tags.karaoke.title">Караоке</title>
                <title lang="en" key="l10n.feature.bar.more_tags.karaoke.title">Karaoke</title>
                <key>karaoke</key>
                <val value="true">
                    <title lang="ru" key="l10n.more_tags.true">Да</title>
                    <title lang="en" key="l10n.more_tags.true">Yes</title>
                </val>
                <val value="false">
                    <title lang="ru" key="l10n.more_tags.false">Нет</title>
                    <title lang="en" key="l10n.more_tags.false">No</title>
                </val>
            </tag>
        </more-tags>
        <icon>restaurant.png</icon>
        <applyed-to>node area</applyed-to>
    </feature>
</doc-part>
