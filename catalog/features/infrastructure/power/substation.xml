<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <feature name="substation">
        <title lang="ru" key="l10n.feature.substation.title">Подстанция</title>
        <title lang="en" key="l10n.feature.substation.title">Substation</title>
        <tags>
            <tag>
                <key>power</key>
                <val value="substation"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <more-tags>
            <tag tag-value-type="boolean">
                <title lang="ru" key="l10n.feature.substation.more_tags.gas_insulated.title">Газовая изоляция</title>
                <title lang="en" key="l10n.feature.substation.more_tags.gas_insulated.title">Gas insulated</title>
                <key>gas_insulated</key>
                <val value="true">
                    <title lang="ru" key="l10n.more_tags.true">Да</title>
                    <title lang="en" key="l10n.more_tags.true">Yes</title>
                </val>
                <val value="false">
                    <title lang="ru" key="l10n.more_tags.false">Нет</title>
                    <title lang="en" key="l10n.more_tags.false">No</title>
                </val>
            </tag>
            <tag tag-value-type="number">
                <title lang="ru" key="l10n.feature.substation.more_tags.voltage.title">Напряжения</title>
                <title lang="en" key="l10n.feature.substation.more_tags.voltage.title">Voltage</title>
                <key>voltage</key>
            </tag>
        </more-tags>
        <icon>substation.png</icon>
        <applyed-to>node area</applyed-to>
    </feature>
</doc-part>
