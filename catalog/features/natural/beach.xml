<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <feature name="beach">
        <title lang="ru" key="l10n.feature.beach.title">Пляж</title>
        <title lang="en" key="l10n.feature.beach.title">Beach</title>
        <description lang="ru" key="l10n.feature.beach.description"/>
        <description lang="en" key="l10n.feature.beach.description"/>
        <tags>
            <tag>
                <key>natural</key>
                <val value="beach"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <trait>surface</trait>
        <more-tags>
            <tag tag-value-type="enum">
                <title lang="ru" key="l10n.feature.beach.more_tags.nudism.title">Нудизм</title>
                <title lang="en" key="l10n.feature.beach.more_tags.nudism.title">Nudism</title>
                <key>nudism</key>
                <val value="yes">
                    <title lang="ru" key="l10n.more_tags.true">Да</title>
                    <title lang="en" key="l10n.more_tags.true">Yes</title>
                </val>
                <val value="no">
                    <title lang="ru" key="l10n.more_tags.false">Нет</title>
                    <title lang="en" key="l10n.more_tags.false">No</title>
                </val>
                <val value="permissive">
                    <title lang="ru" key="l10n.feature.beach.more_tags.nudism.permissive.title">Не возбраняется</title>
                    <title lang="en" key="l10n.feature.beach.more_tags.nudism.permissive.title">Permissive</title>
                </val>
            </tag>
        </more-tags>
        <icon>beach.png</icon>
        <wiki>Естественный пляж</wiki>
        <applyed-to>area</applyed-to>
    </feature>
</doc-part>
