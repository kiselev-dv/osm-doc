<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <feature name="shelter">
        <title lang="ru" key="l10n.feature.shelter.title">Укрытие от непогоды</title>
        <title lang="en" key="l10n.feature.shelter.title">Shelter</title>
        <tags>
            <tag>
                <key>amenity</key>
                <val value="shelter"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <more-tags>
            <tag tag-value-type="boolean">
                <title lang="ru" key="l10n.feature.shelter.more_tags.bench.title">Скамейка</title>
                <title lang="en" key="l10n.feature.shelter.more_tags.bench.title">Bench</title>
                <key>bench</key>
                <val value="true">
                    <title lang="ru" key="l10n.feature.shelter.more_tags.bench.true.title">Да</title>
                    <title lang="en" key="l10n.feature.shelter.more_tags.bench.true.title">Yes</title>
                </val>
                <val value="false">
                    <title lang="ru" key="l10n.feature.shelter.more_tags.bench.false.title">Нет</title>
                    <title lang="en" key="l10n.feature.shelter.more_tags.bench.false.title">No</title>
                </val>
            </tag>
        </more-tags>
        <icon>shelter.png</icon>
        <applyed-to>node area</applyed-to>
    </feature>
</doc-part>
