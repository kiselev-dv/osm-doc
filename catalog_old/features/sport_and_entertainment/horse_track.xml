<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part" xmlns:d="http://map.osm.me/osm-doc-part">
    <feature name="horse_track">
        <title>l10n.feature.horse_track.title</title>
        <tags>
            <tag>
                <title>l10n.feature.horse_track.tags.sport.title</title>
                <key>sport</key>
                <val value="horse_racing"/>
            </tag>
            <tag>
                <key>leisure</key>
                <val value="track"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <trait>seasonal</trait>
        <trait>fee</trait>
        <trait>surface</trait>
        <more-tags>
            <tag d:tag-value-type="boolean">
                <title>l10n.feature.horse_track.more_tags.covered.title</title>
                <key>covered</key>
                <val value="true">
                    <title>l10n.feature.horse_track.more_tags.covered.true.title</title>
                </val>
                <val value="false">
                    <title>l10n.feature.horse_track.more_tags.covered.false.title</title>
                </val>
            </tag>
        </more-tags>
        <icon>horse_track.png</icon>
        <applyed-to>node way area</applyed-to>
    </feature>
</doc-part>
