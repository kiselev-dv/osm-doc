<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part" xmlns:d="http://map.osm.me/osm-doc-part">
    <feature name="bookshop">
        <title>l10n.feature.bookshop.title</title>
        <tags>
            <tag>
                <key>shop</key>
                <val value="books"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <more-tags>
            <tag d:tag-value-type="enum">
                <title>l10n.feature.bookshop.more_tags.second_hand.title</title>
                <key>second_hand</key>
                <val value="yes">
                    <title>l10n.more_tags.true</title>
                </val>
                <val value="no">
                    <title>l10n.more_tags.false</title>
                </val>
                <val value="only">
                    <title>l10n.feature.bookshop.more_tags.second_hand.only.title</title>
                </val>
            </tag>
        </more-tags>
        <icon>bookshop.png</icon>
        <applyed-to>node area</applyed-to>
    </feature>
</doc-part>
