<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part" xmlns:d="http://map.osm.me/osm-doc-part">
    <feature name="pharmacy">
        <title>l10n.feature.pharmacy.title</title>
        <tags>
            <tag>
                <key>amenity</key>
                <val value="pharmacy"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <trait>accessability</trait>
        <more-tags>
            <tag d:tag-value-type="boolean">
                <title>l10n.feature.pharmacy.more_tags.dispensing.title</title>
                <key>dispensing</key>
                <val value="true">
                    <title>l10n.feature.pharmacy.more_tags.dispensing.true.title</title>
                </val>
                <val value="false">
                    <title>l10n.feature.pharmacy.more_tags.dispensing.false.title</title>
                </val>
            </tag>
        </more-tags>
        <icon>pharmacy.png</icon>
        <applyed-to>node area</applyed-to>
    </feature>
</doc-part>
