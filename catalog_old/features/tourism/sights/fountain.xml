<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part" xmlns:d="http://map.osm.me/osm-doc-part">
    <feature name="fountain">
        <title>l10n.feature.fountain.title</title>
        <tags>
            <tag>
                <key>amenity</key>
                <val value="fountain"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <more-tags>
            <tag d:tag-value-type="enum">
                <title>l10n.feature.fountain.more_tags.drinkable.title</title>
                <key>drinkable</key>
                <val value="yes">
                    <title>l10n.more_tags.true</title>
                </val>
                <val value="mineral">
                    <title>l10n.feature.fountain.more_tags.drinkable.mineral.title</title>
                </val>
                <val value="no">
                    <title>l10n.more_tags.false</title>
                </val>
            </tag>
        </more-tags>
        <icon>fountain.png</icon>
        <applyed-to>node area</applyed-to>
    </feature>
</doc-part>
