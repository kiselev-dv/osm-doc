<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part" 
	xmlns:d="http://map.osm.me/osm-doc-part">
    <feature name="artwork">
        <title>l10n.feature.artwork.title</title>
        <tags>
            <tag>
                <key>tourism</key>
                <val value="artwork"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <more-tags>
            <tag>
                <title>l10n.feature.artwork.more_tags.artist_name.title</title>
                <key>artist_name</key>
            </tag>
            <tag d:tag-value-type="date">
                <title>l10n.feature.artwork.more_tags.start_date.title</title>
                <key>start_date</key>
            </tag>
            <tag d:tag-value-type="enum">
                <title>l10n.feature.artwork.more_tags.artwork_type.title</title>
                <key>artwork_type</key>
                <val value="sculpture">
                    <title>l10n.feature.artwork.more_tags.artwork_type.sculpture.title</title>
                </val>
                <val value="statue">
                    <title>l10n.feature.artwork.more_tags.artwork_type.statue.title</title>
                </val>
                <val value="mural">
                    <title>l10n.feature.artwork.more_tags.artwork_type.mural.title</title>
                </val>
                <val value="architecture">
                    <title>l10n.feature.artwork.more_tags.artwork_type.architecture.title</title>
                </val>
                <val value="mosaic">
                    <title>l10n.feature.artwork.more_tags.artwork_type.mosaic.title</title>
                </val>
                <val value="painting">
                    <title>l10n.feature.artwork.more_tags.artwork_type.painting.title</title>
                </val>
            </tag>
        </more-tags>
        <icon>artwork.png</icon>
        <applyed-to>node way area</applyed-to>
    </feature>
</doc-part>
